    "test": "echo \"Error: no tests defined\"",
    "lint": "run-p lint:*",
    "lint:md": "remark .",
    "lint:js": "standard"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "remark-cli": "^6.0.0",
    "remark-lint": "^6.0.3",
    "remark-preset-lint-recommended": "^3.0.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "maven": "^4.4.1"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
